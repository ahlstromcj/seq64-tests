# EXTRA_DIST
#-------------------------------------------------------------------------------

EXTRA_DIST = alsa.m4 \
 ax_prefix_config_h.m4 \
 ax_prog_flex.m4 \
 gcc-version.m4 \
 inttypes.m4 \
 isc-posix.m4 \
 pkg.m4 \
 threadlib.m4 \
 win32msc.m4 \
 xpc_debug.m4 \
